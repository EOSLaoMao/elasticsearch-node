{
  "index_patterns" : ["action_traces*"],
  "settings": {
    "index": {
      "number_of_shards": 5,
      "refresh_interval": -1,
      "number_of_replicas": 0
    }
  },
  "mappings": {
    "_doc": {
      "properties": {
        "trx_id": {
          "type": "keyword"
        },
        "producer_block_id": {
          "type": "keyword"
        },
        "elapsed": {
          "type": "long"
        },
        "context_free": {
          "type": "boolean"
        },
        "console": {
          "type": "text"
        },
        "block_num": {
          "type": "long"
        },
        "block_time": {
          "type": "date"
        },
        "receipt": {
          "enabled": false
        },
        "act": {
          "properties": {
            "name": {
              "type": "keyword"
            },
            "authorization": {
              "enabled": false
            },
            "account": {
              "type": "keyword"
            },
            "hex_data": {
              "enabled": false
            },
            "data": {
              "type": "text"
            }
          }
        },
        "account_ram_deltas": {
          "enabled": false
        }
      }
    }
  }
}