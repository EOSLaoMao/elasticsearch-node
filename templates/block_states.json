{
  "index_patterns" : ["block_states*"],
  "settings": {
    "index": {
      "number_of_shards": 5,
      "refresh_interval": -1,
      "number_of_replicas": 0
    }
  },
  "mappings": {
    "_doc": {
      "properties": {
        "block_header_state": {
          "type": "object",
          "enabled": false
        },
        "block_id": {
          "type": "keyword"
        },
        "block_num": {
          "type": "long"
        },
        "createAt": {
          "type": "date"
        },
        "irreversible": {
          "type": "boolean"
        },
        "validated": {
          "type": "boolean"
        }
      }
    }
  }
}