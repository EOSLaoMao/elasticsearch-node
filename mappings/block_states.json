{
  "settings": {
    "index": {
      "refresh_interval": -1,
      "number_of_replicas": 0,
      "sort": {
        "field" : [ "block_num", "updateAt", "createAt"], 
        "order" : [ "desc", "desc", "desc"],
        "missing": [ "_last", "_last", "_last" ]
      }
    }
  },
  "mappings": {
    "_doc": {
      "properties": {
        "block_header_state": {
          "type": "object",
          "enabled": false
        },
        "block_id": {
          "type": "keyword"
        },
        "block_num": {
          "type": "long"
        },
        "createAt": {
          "type": "date"
        },
        "irreversible": {
          "type": "boolean"
        },
        "updateAt": {
          "type": "date"
        },
        "validated": {
          "type": "boolean"
        }
      }
    }
  }
}