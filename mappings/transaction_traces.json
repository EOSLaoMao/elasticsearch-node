{
  "settings": {
    "index": {
      "refresh_interval": -1,
      "number_of_replicas": 0
    }
  },
  "mappings": {
    "_doc": {
      "properties": {
        "action_traces": {
          "type": "object",
          "enabled": false
        },
        "block_num": {
          "type": "long"
        },
        "block_time": {
          "type": "date"
        },
        "createAt": {
          "type": "date"
        },
        "elapsed": {
          "type": "long"
        },
        "except": {
          "type": "object",
          "enabled": false
        },
        "failed_dtrx_trace": {
          "type": "object",
          "enabled": false
        },
        "id": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        },
        "net_usage": {
          "type": "long"
        },
        "producer_block_id": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        },
        "receipt": {
          "properties": {
            "cpu_usage_us": {
              "type": "long"
            },
            "net_usage_words": {
              "type": "long"
            },
            "status": {
              "type": "text",
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            }
          }
        },
        "scheduled": {
          "type": "boolean"
        },
        "updateAt": {
          "type": "date"
        }
      }
    }
  }
}